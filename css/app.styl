@import 'jeet'

#root
	center(960px)
	.sidebar
		col(1/3)
		background-color: #FFFFCC;
		
		ul
			list-style: none
			li
				display: block
				margin: 0 0 10px 0 

		+below(600px)
			stack()
			ul
				center(200px)
				li
					display: inline	
					margin: 0 15px 0 0

	.main
		col(2/3)
		background-color: #CCCCCC
		+below(600px)
			stack()